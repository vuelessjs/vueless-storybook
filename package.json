{
  "name": "@vueless/storybook",
  "version": "1.1.3-beta.5",
  "description": "Simplifies Storybook configuration for Vueless UI library.",
  "author": "Johnny Grid <hello@vueless.com> (https://vueless.com)",
  "homepage": "https://vueless.com",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "copy": "./bin/index.js"
  },
  "scripts": {
    "lint": "eslint --no-fix src/",
    "lint:fix": "eslint --fix src/",
    "lint:ci": "eslint --no-fix --max-warnings=0",
    "release:prepare": "rm -rf dist && mkdir -p dist && cp -r src/. package.json LICENSE README.md dist/",
    "release:beta": "release-it --ci --npm.publish --preRelease=beta --increment=prerelease",
    "release:patch": "release-it patch --ci --npm.publish --git.tag --github.release",
    "release:minor": "release-it minor --ci --npm.publish --git.tag --github.release",
    "release:major": "release-it major --ci --npm.publish --git.tag --github.release"
  },
  "dependencies": {
    "@storybook/addon-docs": "9.1.3",
    "@storybook/addon-links": "9.1.3",
    "@storybook/addon-themes": "9.1.3",
    "@storybook/vue3-vite": "9.1.3",
    "@vueless/storybook-dark-mode": "^9.0.7",
    "chokidar": "^4.0.3",
    "esbuild": "^0.25.9",
    "globby": "^14.1.0",
    "mkdirp": "^3.0.1",
    "prettier2": "npm:prettier@2.8.8",
    "storybook": "9.1.3",
    "vue-docgen-api": "^4.79.2"
  },
  "devDependencies": {
    "@release-it/bumper": "^7.0.5",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/node": "^24.3.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "9.1.3",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4",
    "tailwindcss": "^4.1.12",
    "vue": "latest",
    "vue-router": "latest",
    "vueless": "^1.2.0"
  },
  "overrides": {
    "vue-docgen-api": {
      "vue": "latest",
      "vue-template-compiler": "latest"
    },
    "storybook": "$storybook"
  },
  "resolutions": {
    "jackspeak": "2.3.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuelessjs/vueless-storybook.git"
  },
  "bugs": {
    "url": "https://github.com/vuelessjs/vueless-storybook/issues"
  }
}
