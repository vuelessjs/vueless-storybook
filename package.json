{
  "name": "@vueless/storybook",
  "version": "1.2.7-beta.2",
  "description": "Simplifies Storybook configuration for Vueless UI library.",
  "author": "Johnny Grid <hello@vueless.com> (https://vueless.com)",
  "homepage": "https://vueless.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "vueless-storybook": "./bin/index.js"
  },
  "scripts": {
    "ts:check": "vue-tsc --build --force",
    "lint": "eslint --no-fix src/",
    "lint:fix": "eslint --fix src/",
    "lint:ci": "eslint --no-fix --max-warnings=0",
    "release:prepare": "rm -rf dist && mkdir -p dist && cp -r src/. package.json LICENSE README.md dist/",
    "release:beta": "release-it --increment=prerelease --preRelease=beta --ci --no-git.tag --no-github.release",
    "release:patch": "release-it patch --ci",
    "release:minor": "release-it minor --ci",
    "release:major": "release-it major --ci"
  },
  "dependencies": {
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-links": "^9.1.10",
    "@storybook/addon-themes": "^9.1.10",
    "@storybook/vue3-vite": "^9.1.10",
    "@vueless/storybook-dark-mode": "^9.0.8",
    "chokidar": "^4.0.3",
    "esbuild": "^0.25.9",
    "globby": "^14.1.0",
    "mkdirp": "^3.0.1",
    "prettier2": "npm:prettier@2.8.8",
    "storybook": "^9.1.10",
    "vue-docgen-api": "^4.79.2"
  },
  "devDependencies": {
    "@release-it/bumper": "^7.0.5",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "9.1.5",
    "prettier": "^3.6.2",
    "release-it": "^19.0.4",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.9",
    "vue": "latest",
    "vue-router": "latest",
    "vue-tsc": "^3.0.6",
    "vueless": "^1.2.5"
  },
  "type": "module",
  "main": "index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "node": "./index.js",
      "types": "./index.d.ts",
      "browser": "./index.browser.js"
    }
  },
  "overrides": {
    "vue-docgen-api": {
      "vue": "latest",
      "vue-template-compiler": "latest"
    },
    "conventional-changelog-conventionalcommits": "8.0.0",
    "storybook": "$storybook"
  },
  "resolutions": {
    "jackspeak": "2.3.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuelessjs/vueless-storybook.git"
  },
  "bugs": {
    "url": "https://github.com/vuelessjs/vueless-storybook/issues"
  }
}
